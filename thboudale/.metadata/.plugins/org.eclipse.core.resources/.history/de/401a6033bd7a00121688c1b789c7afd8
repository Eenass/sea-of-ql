public class TestExprChecking {
	private ExprChecker exprVisitor;
	private Map<String,Type> typeEnv;
	private List<String> errMsgs;
	
	@Test
	public void testExprChecking() {
		assertEquals(true, exprVisitor.visit(new Add(new Int(10), new Int(25))));
	}
}