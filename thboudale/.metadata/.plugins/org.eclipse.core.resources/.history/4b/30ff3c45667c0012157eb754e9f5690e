package org.uva.sea.ql.ast.statements;

import java.util.ArrayList;
import org.uva.sea.ql.visitor.FrmStmtVisitor;

public class BlockOfStatements extends AStatement {
	private final ArrayList<AStatement> blStmts = new ArrayList<AStatement>();
	
	public void addStatement(AStatement stmt) {
		blStmts.add(stmt);
	}
	
	public ArrayList<AStatement> getStmts() {
		return blStmts;
	}
	
	@Override
	public <T> T accept(FrmStmtVisitor<T> visitor) {
		return visitor.visit(this);		
	}
}