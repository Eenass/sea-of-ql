package org.uva.sea.ql.ast.form;

import org.uva.sea.ql.ast.expressions.Ident;
import org.uva.sea.ql.ast.statements.BlockOfStatements;
import org.uva.sea.ql.visitor.FrmStmtVisitor;

public class Form extends AForm {
	private final Ident ident;
	private final BlockOfStatements bStms;
	
	public Form(Ident ident, BlockOfStatements bStms) {
		this.ident = ident;
		this.bStms = bStms;
	}
	
	public Ident getIdent() {
		return ident;
	}
	
	public BlockOfStatements getStms() {
		return bStms;
	}

	@Override
	public <T> T accept(FrmStmtVisitor<T> visitor) {
		return visitor.visit(this);		
	}
}