package org.uva.sea.ql.ast.expression;

import org.uva.sea.ql.ast.ASTNode;
import org.uva.sea.ql.visitors.Environment;
import org.uva.sea.ql.visitors.TypeChecker;

public abstract class Expr implements ASTNode {
	public abstract Expr accept(Evaluator eval, Environment env);
	public abstract Type accept(TypeChecker type, Environment env);
}
