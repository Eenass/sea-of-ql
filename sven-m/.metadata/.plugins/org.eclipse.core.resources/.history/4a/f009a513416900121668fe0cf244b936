package org.uva.sea.ql.ast.form;

import java.util.List;

import org.uva.sea.ql.ast.ASTNode;

public class Body implements ASTNode/*, List<FormElement>*/ {
	protected final FormElement[] formElements;
	
	public Body() {
		formElements = new FormElement[0];
	}
	
	public Body(List<FormElement> formElements) {
		this.formElements = formElements.toArray(new FormElement[0]);
	}
}
