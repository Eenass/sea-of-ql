package org.uva.sea.ql.ast;

import java.util.AbstractList;
import java.util.List;

public class Body implements ASTNode, List<FormElement> {
	protected final AbstractList<FormElement> formElements;
	
	public Body() {
		formElements = new AbstractList<FormElement>();
	}
	
	public Body(List<FormElement> formElements) {
		this.formElements = new AbstractList<FormElement>(formElements);
	}
}
