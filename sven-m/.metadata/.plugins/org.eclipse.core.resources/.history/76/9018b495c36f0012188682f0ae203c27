package org.uva.sea.ql.ast.form;

import java.util.List;
import java.util.Map;

import org.uva.sea.ql.ast.ASTNode;
import org.uva.sea.ql.ast.type.ExprType;
import org.uva.sea.ql.ast.type.IFormElementTypeChecker;

public class Body implements ASTNode {
	private final List<FormElement> formElements;
	
	public Body(List<FormElement> formElements) {
		this.formElements = formElements;
	}
	
	public List<FormElement> getElements() {
		return formElements;
	}

	public void accept(IFormElementTypeChecker visitor, Map<String, ExprType> typeEnv) {
		visitor.visit(this, typeEnv);
	}
}
