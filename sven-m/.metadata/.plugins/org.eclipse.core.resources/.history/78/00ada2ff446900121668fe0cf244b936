package org.uva.sea.ql.parser.test;

import static org.junit.Assert.assertEquals;

import org.junit.Test;
import org.uva.sea.ql.ast.form.Body;

public class BodyTest extends SyntaxTest {

	@Override
	protected Class<?> parse(String src) throws ParseError {
		return getParser().parseBody(src).getClass();
	}
	
	@Test
	public void TestBodies() throws ParseError {
		assertEquals(parse("{ \n fdsafjdks: \" \" boolean }"), Body.class);
		assertEquals(parse("{ \n {}:'FFD<> \" \" boolean }"), Body.class);
		assertEquals(parse("{ \n @#$%^&: \" \" boolean }"), Body.class);
		assertEquals(parse("{ \n \t \t \r\n \r \r \n }"), Body.class);
		assertEquals(parse("{ }"), Body.class);
	}
}
