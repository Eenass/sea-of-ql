package org.uva.sea.ql.ast.type;

import org.uva.sea.ql.ast.visitor.TypeVisitor;

public class StringConverter implements TypeVisitor<String> {

	@Override
	public String visit(BoolType type) {
		return "Bool";
	}

	@Override
	public String visit(StrType type) {
		return "String";
	}

	@Override
	public String visit(IntType type) {
		return "Int";
	}

}
